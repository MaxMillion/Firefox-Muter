<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:em="http://www.mozilla.org/2004/em-rdf#">
<Description about="urn:mozilla:install-manifest">
  
  <em:id>muter@yxl.name</em:id>
  <em:name>Muter</em:name>
  <em:type>2</em:type>
  <em:version>0.9.5</em:version>
  <em:description>Let you mute the Firefox browser.</em:description> 
  <em:creator>Yuan Xulei</em:creator>
  <em:contributor>Zhao Botong</em:contributor>
  <em:homepageURL>http://yxl.github.com/Firefox-Muter</em:homepageURL>
  <em:optionsURL>chrome://muter/content/muterSettings.xul</em:optionsURL>
  <em:iconURL>chrome://muter/skin/icon.png</em:iconURL>
  <em:unpack>true</em:unpack>
  <em:targetPlatform>WINNT</em:targetPlatform>
  <em:targetPlatform>Linux</em:targetPlatform>
  
  <!-- Firefox -->
  <em:targetApplication>
    <Description>
      <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
      <em:minVersion>3.6</em:minVersion>
      <em:maxVersion>13.0a1</em:maxVersion>
    </Description>
  </em:targetApplication>
  
  <!-- SeaMonkey -->
  <em:targetApplication>
    <Description>
      <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
      <em:minVersion>2.1</em:minVersion>
      <em:maxVersion>2.7</em:maxVersion>
    </Description>
  </em:targetApplication>
  
  <em:localized>
    <Description>
      <em:locale>zh-CN</em:locale>
      <em:name>静音</em:name>
      <em:description>提供浏览器静音功能 </em:description>
    </Description>
  </em:localized>
  
  <em:localized>
    <Description>
      <em:locale>zh-TW</em:locale>
      <em:name>靜音</em:name>
      <em:description>提供瀏覽器靜音功能 </em:description>
    </Description>
  </em:localized>

  <em:localized>
    <Description>
      <em:locale>ja-JP</em:locale>
      <em:name>消音</em:name>
      <em:description>Firefoxから流れる音だけをワンタッチで消音できるアドオン「消音」
</em:description>
    </Description>
  </em:localized>
  
</Description>
</RDF> 
